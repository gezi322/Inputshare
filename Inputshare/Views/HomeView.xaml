<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Inputshare.Views.HomeView">
  <StackPanel Orientation="Vertical">
    <Button Content="Server" Command="{Binding CommandServer}"/>
    <Button Content="Client" Command="{Binding CommandClient}"/>
    <Button Content="Service client" Command="{Binding CommandWinService}" IsVisible="{Binding ShowWinService}"/>
  </StackPanel>
</UserControl>
